body {
  background-color:#e6e6f0;
  /* background-color: #333333; */
  color: #fff;
  font-family: "Poppins", sans-serif;
}
.App {
  min-height: 100vh;
  display: flex;
}

.visible {
  margin-left: 0;
}
.hidden {
  margin-left: -300px; /* Adjust the value based on your sidebar width */
}
.main {
  flex: 9;
}

.btn {
  margin-bottom: 30px;
  margin: 20px;
}
.brand {
  font-size: 2rem;
}

.lowerSide {
  margin-top: -45px;
  width: 500px;
  padding: 10px;
}
.main {
  min-height: calc(100vh - 14rem);
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 6rem 10rem;
  margin-bottom: 0;
}
.chatFooter {
  margin-top: auto;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.inputBox > input {
  width: calc(100% - 3rem);
  outline: none;
  padding: 1.25rem;
  color: black;
}
.inputBox > input,
.send {
  background-color: transparent;
  border: none;
}
.chatFooter > p {
  margin: 2rem 0;
}
.icon {
  margin-right: 1rem;
}

